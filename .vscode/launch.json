{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "C/C++ Runner: Debug Session",
      "type": "cppdbg",
      "request": "launch",
      "args": ["<", "${workspaceFolder}/input_map_3.json", ">", "${workspaceFolder}/output_map.json"],
      "stopAtEntry": false,
      "externalConsole": true,
      "cwd": "d:/Artem/Cpp-Course/cpp-transport-catalogue/transport-catalogue",
      "program": "d:/Artem/Cpp-Course/cpp-transport-catalogue/transport-catalogue/build/Debug/outDebug",
      "MIMode": "gdb",
      "miDebuggerPath": "gdb",
      "setupCommands": [
        {
          "description": "Enable pretty-printing for gdb",
          "text": "-enable-pretty-printing",
          "ignoreFailures": true
        }
      ]       
    }
  ]
}